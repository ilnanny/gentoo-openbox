<?xml encoding="UTF-8" version="1.0"?>
<actions>
<action>
	<icon>utilities-terminal</icon>
	<name>Apri un Terminale </name>
	<unique-id>1519087650955555-2</unique-id>
	<command>exo-open --working-directory %f --launch TerminalEmulator</command>
	<description></description>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
</action>
<action>
	<icon>emblem-symbolic-link</icon>
	<name>Collegamento</name>
	<unique-id>1556791377792315-1</unique-id>
	<command>ln -s %f &quot;symlink a %n&quot;</command>
	<description>crea un collegamento simbolico</description>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon>dialog-password</icon>
	<name>Super Utente</name>
	<unique-id>1556820005359575-3</unique-id>
	<command>sudo thunar %f</command>
	<description></description>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon>editcopy</icon>
	<name>Copia in</name>
	<unique-id>1518979542113323-3</unique-id>
	<command>cp -a -r  %F $(zenity --file-selection --directory)</command>
	<description></description>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon>folder-move</icon>
	<name>Sposta in</name>
	<unique-id>1518414274287792-5</unique-id>
	<command>mv %F $(zenity --file-selection --directory)</command>
	<description></description>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon>shellscript</icon>
	<name>Rendi Eseguibile</name>
	<unique-id>1556819970607542-2</unique-id>
	<command>chmod +x %f</command>
	<description></description>
	<patterns>*.sh;*.py</patterns>
	<directories/>
	<other-files/>
	<text-files/>
</action>
<action>
	<icon>printer-printing</icon>
	<name>Stampa</name>
	<unique-id>1480694167418860-1</unique-id>
	<command>xfprint4 %f</command>
	<description></description>
	<patterns>*.pdf;*.doc;*.txt;*.docx;*.odt;*.jpeg;*.gif;*.jpg;*.png;*.ttf;*.xcf;*.ai;*.psd</patterns>
	<image-files/>
	<other-files/>
	<text-files/>
</action>
<action>
	<icon>preferences-system-search</icon>
	<name>Cerca </name>
	<unique-id>1556820027415523-4</unique-id>
	<command>catfish --fileman=thunar --path=%f --hidden --method=locate</command>
	<description>Ricerca di un file in una directory</description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>gnome-music</icon>
	<name>Esegui Audio </name>
	<unique-id>1525076054397861-1</unique-id>
	<command>audacious %F</command>
	<description>Riproduci Audio e Cartelle con Gogglesmm</description>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<other-files/>
</action>
<action>
	<icon>vlc</icon>
	<name>Esegui Video</name>
	<unique-id>1518414429431959-9</unique-id>
	<command>vlc %F</command>
	<description>Riproduci Video  con VLC</description>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<video-files/>
</action>
<action>
	<icon>terminal</icon>
	<name>Esegui Script</name>
	<unique-id>1556819956847486-1</unique-id>
	<command>xfterm -e %f --hold</command>
	<description>esegue script</description>
	<patterns>*</patterns>
	<other-files/>
</action>
<action>
	<icon>folder-remote</icon>
	<name>Condividi </name>
	<unique-id>1518414450696004-10</unique-id>
	<command>net usershare add %n %f “” Everyone:F guest_ok=n &amp;&amp; chmod 777 %f</command>
	<description>Condivide la cartella in LAN</description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>archive-extract</icon>
	<name>Estrai  Zip</name>
	<unique-id>1518324722592450-1</unique-id>
	<command>unzip \*.zip</command>
	<description></description>
	<patterns>*.zip</patterns>
	<directories/>
	<other-files/>
</action>
<action>
	<icon>archive-insert</icon>
	<name>Estrai Rar</name>
	<unique-id>1530524871999921-1</unique-id>
	<command>unrar \*.rar</command>
	<description></description>
	<patterns>*.rar</patterns>
	<directories/>
	<other-files/>
</action>
<action>
	<icon>system-devices-panel</icon>
	<name>MD5 e SHA</name>
	<unique-id>1521425314397498-2</unique-id>
	<command>zenity --info --title=&quot;%n&quot; --text=&quot;%nnn$(md5sum) MD5nn$(sha1sum) SHA1nn$(sha256sum) SHA256&quot;</command>
	<description></description>
	<patterns>*.pdf;*.doc;*.txt;*.docx;*.odt;*.iso</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon>gnome-mime-image-jpeg</icon>
	<name>PNG 2 .JPG</name>
	<unique-id>1552369276813551-4</unique-id>
	<command>convert %f -quality 95 %f.jpg</command>
	<description>converte immagini png in jpg</description>
	<patterns>*.png</patterns>
	<image-files/>
</action>
<action>
	<icon>Adobeillustrator</icon>
	<name>AI 2 SVG</name>
	<unique-id>1552593441814889-3</unique-id>
	<command>for i in *; do inkscape $i --export-plain-svg=`echo $i | sed -e s/ai$/svg/`; done
</command>
	<description>esporta da ai a svg</description>
	<patterns>*.ai</patterns>
	<image-files/>
</action>
<action>
	<icon>image-png</icon>
	<name>SVG 2 PNG</name>
	<unique-id>1556426909041201-1</unique-id>
	<command>for i in *; do inkscape $i -d=300 -C --export-png=`echo $i | sed -e s/svg$/png/`; done</command>
	<description>esporta da svg a png</description>
	<patterns>*.svg</patterns>
	<image-files/>
</action>
</actions>
